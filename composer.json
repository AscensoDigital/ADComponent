{
    "name": "ascensodigital/component-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle con extensiones de form twig y doctrine",
    "keywords": ["bundle extensiones"],
    "homepage": "http://ascensodigital.github.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Claudio Corvalan",
            "email": "corvalanclaudio7@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.1",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/security-bundle": "^4.0|^5.0",
        "symfony/form": "^4.0|^5.0",
        "twig/twig": "^2.5|^3.0",
        "symfony/validator": "^4.0|^5.0",
        "doctrine/annotations": "^1.6",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": { "AscensoDigital\\ComponentBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AscensoDigital\\ComponentBundle\\Tests\\": "Tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "version": "2.8.2",
    "require-dev": {
        "doctrine/orm": "^2.6"
    }
}
